    
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
; add [r0,r1] with [r2,r3]. Store result in [r4,r5]
; [1, 100001] + [2, 100050] = [4, 00051]	
; expected: r4=4 r5=51
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

	.=1000
	mov #1, r0
	mov #100001, r1
	mov #2, r2
	mov #100050, r3
	
	mov r2, r4
	mov r3, r5
	add r1, r5
	adc r4
	add r0, r4
	halt
